<h1> Links </h1>
<a href="/links/new">Add Bookmark</a><br>
<a href="/tags/bubbles">Tags with bubbles</a>
<br>
<ul id='links'>
  <% @links.each do |link| %>
  <li>
    Title: <%= link.title %>
    URL: <%= link.url %>
    Tags: <% link.tags.each do |x| %>
              <%=x.name%>
          <% end %>
  </li>
  <% end %>
</ul>
